{
  "name": "tabletop-model-collector",
  "version": "1.0.0",
  "description": "An application to store and manage information about models from various armies and tabletop gaming systems. Features include a detailed collection view, bulk CSV import/export, and settings to customize game systems and armies.",
  "private": true,
  "scripts": {
  "dev": "vite",
  "start": "vite preview",
  "build": "tsc && vite build",
  "preview": "vite preview",
  "serve-dist": "node ./scripts/serve-dist-node.js 5000",
  "serve-dist:win": "powershell -ExecutionPolicy Bypass -File ./scripts/serve-dist.ps1 -Port 5000"
  },
  "dependencies": {
    "@google/genai": "^1.25.0",
    "http-server": "^14.1.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "run": "^1.5.0"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^6.4.1"
  }
}
